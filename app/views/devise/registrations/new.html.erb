<div class="container">
<%= simple_form_for(resource, :as => resource_name, :url => registration_path(resource_name), html:{class: "well span6 offset3"}) do |f| %>
<fieldset>
<legend >Sign Up</legend>  
<%= f.input :first_name%>
<%= f.input :last_name %>
<%= f.input :email %>
<%= f.input :password %>

<%= f.button :submit, "Sign up" %>
</fieldset>
<% end %>
</div>


