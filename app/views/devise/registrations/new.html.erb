<h2>Sign up</h2>

<%= simple_form_for(resource, :as => resource_name, :url => registration_path(resource_name), html:{class: "well"}) do |f| %>


<fieldset>
<legend>Sign In</legend>  
<%= f.input :first_name %>
<%= f.input :last_name %>
<%= f.input :email %>
<%= f.input :password %>

<%= f.button :submit, "Sign up" %>
</fieldset>
<% end %>

<%= render "devise/shared/links" %>

