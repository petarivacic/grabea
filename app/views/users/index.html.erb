<% provide(:title, 'All users') %>
<h1>All users</h1>


<%= will_paginate %>
<ul class="users">
	<% @users.each do |user| %>
		<li>
			<%= image_tag user.gravatar_url, style: "width: 50px; height: 50px;",  class: "img-rounded" %>
			<%= link_to user.full_name, user %>
		</li>
	<% end %>
</ul>
<%= will_paginate %>



